			result.models[#result.models + 1] = {
				id = 'lollo_freestyle_train_station/railroad/flatSides/passengers/era_c_large/lollo_side.mdl',
				slotId = slotId,
				transf = buildingTransf,
				tag = tag
			}
			local perronModelId = 'lollo_freestyle_train_station/railroad/flatSides/passengers/era_c_large/lollo_perron.mdl'
